name: TestForMac

on: 
  push:
    branches: [main, master]
  
jobs:
  Hello_Mac:
    runs-on: macos-latest
    steps:
    - name: Run a simple command
      run: "echo hello mac"
      
  
  PlayWithPowerShell:
    runs-on: macos-latest
    steps:
    - name: Run a power shell command
      run: $PSVersionTable
 
